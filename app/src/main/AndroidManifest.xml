<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hidee.popularmovie">

    <application
        android:name=".CardView.AppController"
        android:allowBackup="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Deskripsi_Film.Activity_Deskripsi_Film"
            android:parentActivityName=".CardView.MainActivityAlbum"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CardView.MainActivityAlbum"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Deskripsi_Film.MainActivity"
            android:parentActivityName=".CardView.MainActivityAlbum"/>


    </application>

</manifest>